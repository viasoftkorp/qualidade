                  "maximumError": "10kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/controle-tratamento-termico/src/environments/environment.ts",
                  "with": "apps/controle-tratamento-termico/src/environments/environment.mock.ts"
                }
              ],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "dev-env": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/controle-tratamento-termico/src/environments/environment.ts",
                  "with": "apps/controle-tratamento-termico/src/environments/environment.dev-env.ts"
                }
              ],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "dev-env"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "buildTarget": "controle-tratamento-termico-portal:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "controle-tratamento-termico-portal:build:production"
            },
            "mock": {
              "buildTarget": "controle-tratamento-termico:build:mock"
            },
            "dev-env": {
              "buildTarget": "controle-tratamento-termico:build:dev-env"
            }
          },
          "defaultConfiguration": "dev-env"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "controle-tratamento-termico-portal:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "apps/controle-tratamento-termico/**/*.ts",
              "apps/controle-tratamento-termico/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}