{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "libs",
  "projects": {
    "inspecao-saida": {
      "root": "apps/inspecao-saida",
      "sourceRoot": "apps/inspecao-saida/src",
      "projectType": "application",
      "prefix": "qa",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/inspecao-saida",
            "index": "apps/inspecao-saida/src/index.html",
            "main": "apps/inspecao-saida/src/main.ts",
            "polyfills": "apps/inspecao-saida/src/polyfills.ts",
            "tsConfig": "apps/inspecao-saida/tsconfig.app.json",
            "assets": [
              "apps/inspecao-saida/src/favicon.ico",
              "apps/inspecao-saida/src/assets"
            ],
            "styles": [
              "apps/inspecao-saida/src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/@viasoft/fontawesome-pro/css/all.min.css",
              "node_modules/@viasoft/fontawesome-pro/css/light.min.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "dev-env": {
              "fileReplacements": [
                {
                  "replace": "apps/inspecao-saida/src/environments/environment.ts",
                  "with": "apps/inspecao-saida/src/environments/environment.dev-env.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/inspecao-saida/src/environments/environment.ts",
                  "with": "apps/inspecao-saida/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "inspecao-saida:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "inspecao-saida:build:production"
            },
            "dev-env": {
              "browserTarget": "inspecao-saida:build:dev-env"
          }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "inspecao-saida:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/inspecao-saida/src/test.ts",
            "polyfills": "apps/inspecao-saida/src/polyfills.ts",
            "tsConfig": "apps/inspecao-saida/tsconfig.spec.json",
            "karmaConfig": "apps/inspecao-saida/karma.conf.js",
            "assets": [
              "apps/inspecao-saida/src/favicon.ico",
              "apps/inspecao-saida/src/assets"
            ],
            "styles": [
              "apps/inspecao-saida/src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/@viasoft/fontawesome-pro/css/all.min.css",
              "node_modules/@viasoft/fontawesome-pro/css/light.min.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/inspecao-saida/tsconfig.app.json",
              "apps/inspecao-saida/tsconfig.spec.json",
              "apps/inspecao-saida/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/inspecao-saida/e2e/protractor.conf.js",
            "devServerTarget": "inspecao-saida:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "inspecao-saida:serve:production"
            }
          }
        }
      }
    },
    "inspecao-saida-portal": {
      "root": "apps/inspecao-saida",
      "sourceRoot": "apps/inspecao-saida/src",
      "projectType": "application",
      "prefix": "qa",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/inspecao-saida",
            "index": "apps/inspecao-saida/src/index.html",
            "main": "apps/inspecao-saida/src/main.single-spa.ts",
            "polyfills": "apps/inspecao-saida/src/polyfills.ts",
            "tsConfig": "apps/inspecao-saida/tsconfig.app.json",
            "assets": [
              "apps/inspecao-saida/src/favicon.ico",
              "apps/inspecao-saida/src/assets"
            ],
            "styles": [
              "apps/inspecao-saida/src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/@viasoft/fontawesome-pro/css/all.min.css",
              "node_modules/@viasoft/fontawesome-pro/css/light.min.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "customWebpackConfig": {
              "path": "apps/inspecao-saida/extra-webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/inspecao-saida/src/environments/environment.ts",
                  "with": "apps/inspecao-saida/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "inspecao-saida-portal:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "inspecao-saida-portal:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "inspecao-saida-portal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/inspecao-saida/src/test.ts",
            "polyfills": "apps/inspecao-saida/src/polyfills.ts",
            "tsConfig": "apps/inspecao-saida/tsconfig.spec.json",
            "karmaConfig": "apps/inspecao-saida/karma.conf.js",
            "assets": [
              "apps/inspecao-saida/src/favicon.ico",
              "apps/inspecao-saida/src/assets"
            ],
            "styles": [
              "apps/inspecao-saida/src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/@viasoft/fontawesome-pro/css/all.min.css",
              "node_modules/@viasoft/fontawesome-pro/css/light.min.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/inspecao-saida/tsconfig.app.json",
              "apps/inspecao-saida/tsconfig.spec.json",
              "apps/inspecao-saida/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/inspecao-saida/e2e/protractor.conf.js",
            "devServerTarget": "inspecao-saida:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "inspecao-saida:serve:production"
            }
          }
        }
      }
    },
    "qualidade-inspecao-saida": {
      "projectType": "library",
      "root": "libs/qualidade-inspecao-saida",
      "sourceRoot": "libs/qualidade-inspecao-saida/src",
      "prefix": "qa",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "libs/qualidade-inspecao-saida/tsconfig.lib.json",
            "project": "libs/qualidade-inspecao-saida/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/qualidade-inspecao-saida/src/test.ts",
            "tsConfig": "libs/qualidade-inspecao-saida/tsconfig.spec.json",
            "karmaConfig": "libs/qualidade-inspecao-saida/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/qualidade-inspecao-saida/tsconfig.lib.json",
              "libs/qualidade-inspecao-saida/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "inspecao-saida"
}